{"ast":null,"code":"import { validationMixin } from \"vuelidate\";\nimport { required, minLength, email } from \"vuelidate/lib/validators\";\nexport default {\n  name: \"formsVuelidate\",\n  mixins: [validationMixin],\n  data() {\n    return {\n      form: {\n        login: \"\",\n        email: \"\",\n        password: \"\",\n        country: \"\",\n        favoiriteThemes: [\"IT\"],\n        agreeWithSendEmail: [],\n        gender: \"female\"\n      },\n      countries: [{\n        label: \"Ukraine\",\n        value: \"Ukraine\"\n      }, {\n        label: \"Poland\",\n        value: \"Poland\"\n      }, {\n        label: \"France\",\n        value: \"France\"\n      }],\n      themes: [{\n        label: \"Технології\",\n        value: \"IT\"\n      }, {\n        label: \"Мови\",\n        value: \"languages\"\n      }, {\n        label: \"Математика\",\n        value: \"mathematics\"\n      }]\n    };\n  },\n  validations: {\n    form: {\n      login: {\n        required,\n        minLength: minLength(8)\n      },\n      email: {\n        required,\n        email\n      },\n      password: {\n        required\n      }\n    }\n  },\n  methods: {\n    checkForm() {\n      this.$v.from.$touch();\n      if (this.$v.form.$error) {\n        console.log(\"Validation was successful\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["validationMixin","required","minLength","email","name","mixins","data","form","login","password","country","favoiriteThemes","agreeWithSendEmail","gender","countries","label","value","themes","validations","methods","checkForm","$v","from","$touch","$error","console","log"],"sources":["src/components/FormsVuelidate.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>Vuelidate</h2>\n    <form @submit.prevent=\"checkForm\">\n      <div>\n        <label for=\"login\">Login:</label>\n        <input\n          id=\"login\"\n          :class=\"$v.form.login.$error ? 'is-invalid' : ''\"\n          v-model.trim=\"form.login\"\n        />\n        <p v-if=\"$v.form.login.$dirty && !$v.form.login.required\">Required</p>\n        <p v-if=\"$v.form.login.$dirty && !$v.form.login.minLength\">\n          There must be more than five characters\n        </p>\n      </div>\n      <div>\n        <label for=\"login\">Email:</label>\n        <input\n          id=\"email\"\n          type=\"email\"\n          v-model.trim=\"form.email\"\n          :class=\"$v.form.email.$error ? 'is-invalid' : ''\"\n        />\n        <p v-if=\"$v.form.login.$dirty && !$v.form.email.required\">Required</p>\n        <p v-if=\"$v.form.login.$dirty && !$v.form.email.email\">\n          Email is not required\n        </p>\n      </div>\n      <div>\n        <label for=\"login\">Password:</label>\n        <input id=\"password\" type=\"password\" v-model.trim=\"form.password\" />\n        <p v-if=\"$v.form.login.$dirty && !$v.form.password.required\">\n          Required\n        </p>\n        <p v-if=\"$v.form.login.$dirty && !$v.form.password.password\">\n          Password is not required\n        </p>\n      </div>\n      <div>\n        <label for=\"country\">Country of residence:</label>\n        <select id=\"country\" v-model=\"form.country\">\n          <option\n            v-for=\"(country, index) in countries\"\n            :value=\"country.value\"\n            :key=\"index\"\n          >\n            {{ country.label }}\n          </option>\n        </select>\n      </div>\n      <div>\n        <label for=\"themes\">Favorite themes:</label>\n        <select id=\"themes\" v-model=\"form.favoiriteThemes\" multiple>\n          <option\n            v-for=\"(theme, index) in themes\"\n            :value=\"theme.value\"\n            :key=\"index\"\n          >\n            {{ theme.label }}\n          </option>\n        </select>\n      </div>\n\n      <div>\n        <input\n          value=\"1\"\n          type=\"checkbox\"\n          id=\"notification\"\n          v-model=\"form.agreeWithSendEmail\"\n        />\n        <label for=\"notification\">Notify me about new courses</label>\n      </div>\n      <div>\n        <input\n          value=\"2\"\n          type=\"checkbox\"\n          id=\"notification2\"\n          v-model=\"form.agreeWithSendEmail\"\n        />\n        <label for=\"notification2\">Notify me about new courses 2</label>\n      </div>\n\n      <div>\n        <div>\n          <input\n            value=\"male\"\n            type=\"radio\"\n            name=\"exampleRadios\"\n            id=\"male\"\n            v-model=\"form.gender\"\n          />\n          <label for=\"male\">Male</label>\n        </div>\n        <div>\n          <input\n            value=\"female\"\n            type=\"radio\"\n            name=\"exampleRadios\"\n            id=\"female\"\n            v-model=\"form.gender\"\n          />\n          <label for=\"female\">Female</label>\n        </div>\n      </div>\n\n      <button type=\"submit\">Save</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport { validationMixin } from \"vuelidate\";\nimport { required, minLength, email } from \"vuelidate/lib/validators\";\nexport default {\n  name: \"formsVuelidate\",\n  mixins: [validationMixin],\n  data() {\n    return {\n      form: {\n        login: \"\",\n        email: \"\",\n        password: \"\",\n        country: \"\",\n        favoiriteThemes: [\"IT\"],\n        agreeWithSendEmail: [],\n        gender: \"female\",\n      },\n      countries: [\n        { label: \"Ukraine\", value: \"Ukraine\" },\n        { label: \"Poland\", value: \"Poland\" },\n        { label: \"France\", value: \"France\" },\n      ],\n      themes: [\n        { label: \"Технології\", value: \"IT\" },\n        { label: \"Мови\", value: \"languages\" },\n        { label: \"Математика\", value: \"mathematics\" },\n      ],\n    };\n  },\n  validations: {\n    form: {\n      login: { required, minLength: minLength(8) },\n      email: { required, email },\n      password: { required },\n    },\n  },\n  methods: {\n    checkForm() {\n      this.$v.from.$touch();\n      if (this.$v.form.$error) {\n        console.log(\"Validation was successful\");\n      }\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"],"mappings":"AAgHA,SAAAA,eAAA;AACA,SAAAC,QAAA,EAAAC,SAAA,EAAAC,KAAA;AACA;EACAC,IAAA;EACAC,MAAA,GAAAL,eAAA;EACAM,KAAA;IACA;MACAC,IAAA;QACAC,KAAA;QACAL,KAAA;QACAM,QAAA;QACAC,OAAA;QACAC,eAAA;QACAC,kBAAA;QACAC,MAAA;MACA;MACAC,SAAA,GACA;QAAAC,KAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,KAAA;MAAA,EACA;MACAC,MAAA,GACA;QAAAF,KAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,KAAA;MAAA;IAEA;EACA;EACAE,WAAA;IACAX,IAAA;MACAC,KAAA;QAAAP,QAAA;QAAAC,SAAA,EAAAA,SAAA;MAAA;MACAC,KAAA;QAAAF,QAAA;QAAAE;MAAA;MACAM,QAAA;QAAAR;MAAA;IACA;EACA;EACAkB,OAAA;IACAC,UAAA;MACA,KAAAC,EAAA,CAAAC,IAAA,CAAAC,MAAA;MACA,SAAAF,EAAA,CAAAd,IAAA,CAAAiB,MAAA;QACAC,OAAA,CAAAC,GAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}